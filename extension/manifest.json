{
  "manifest_version": 3,
  "name": "Anti-Fingerprinting Extension",
  "version": "1.0",
  "description": "Blocks and spoofs browser fingerprinting techniques.",
  "permissions": [
    "scripting",
    "storage",
    "declarativeNetRequest",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScripts/inject_Canvas.js",
      "contentScripts/inject_WebGL.js",
      "contentScripts/inject_Audio.js",
      "contentScripts/inject_Fonts.js",
      "contentScripts/inject_Detection.js"],
      "run_at": "document_start",
      "world": "MAIN"
      
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
